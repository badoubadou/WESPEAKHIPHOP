	li a
		bottom 0
		font-size .65em
		color #000
		position absolute
		top 0
		width 100%
		align_middle()
		font-weight 200
		&:hover
		&.selected
			color #000
			.warp_contry .txt
				font-weight 900
			
		span.txt
			display none
		span.dot
			background transparent
			display block
			width 40px
			height 40px
			border-radius 100%
			position relative
			&:after
				transition all .3s ease
				pseudo_full($grey, 1, 4px, 4px )
				center()
				border-radius 100%
				
		.warp_contry
			display none	
			font-size 1em
			font-weight normal
			position relative
			@media $landscape_smallwidth
				display block
				.contry
					position relative
					top 2px			
					.txt
						position relative
					&:after
						pseudo_full($yellow)
						width calc( 100% + 10px )
						transition all .5s ease
						border-radius 5px
						opacity 0
						z-index -1
						margin 5px
						height 16px
						top -9px
						left -10px
						
			@media $mobile_landscape
				display none
				
						
		&:hover
		&.selected
			font-weight 700
			span.dot:after
				width 10px
				height 10px
				background #000
				
			.warp_contry
				font-weight 200
				.contry:after
					opacity 1
				
		@media $landscape_smallwidth
			span.dot
				display none
			span.txt
				display inline-block
				line-height 1.1em
				
		@media $mobile_landscape
			span.txt
				display none
			span.dot
				background transparent
				display block
				width 40px
				height 40px
				border-radius 100%
				position relative
				margin-left -18px
				margin-top -18px
				&:after
					pseudo_full($grey, 1, 4px, 4px )
					center()
					border-radius 100%
					
				
