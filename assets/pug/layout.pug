block data

- var all_videos = []
each it in WSH.channel.item
	if it.post_type == 'videos'
		- all_videos.push(it) 

- var videos = []
each video in all_videos
	if video.category
		each cat in video.category
			if cat.nicename == langage
				- videos.push(video) 


- videos.sort(function(a, b) {return a.menu_order - b.menu_order;})

